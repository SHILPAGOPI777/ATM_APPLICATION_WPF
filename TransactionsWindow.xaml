<Window x:Class="ATM_Application_WPF.TransactionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Transaction History" Height="400" Width="400">
    <Grid Background="LightBlue">
        <StackPanel Margin="10">
            <TextBlock Text="Transaction History" FontSize="16" FontWeight="Bold" HorizontalAlignment="Center"/>
            <ListBox Name="TransactionsListBox"/>
            <Button Name="CloseButton" Content="Close" Click="CloseButton_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
